# ClickUp API Token (получить можно в ClickUp -> Settings -> Apps -> API Token)
CLICKUP_TOKEN=your_token_here

# ID вашей команды (можно найти в URL или через API)
TEAM_ID=your_team_id_here

# Финансовые настройки
HOURLY_RATE=25
CURRENCY=USD
TAX_RATE=0.2

# Настройки отображения
SHOW_TIME_ENTRIES=true
SHOW_DAILY_BREAKDOWN=false
SHOW_COST=true
